{
  "name": "cryptobot",
  "version": "3.0.0",
  "description": "Cryptocurrency trading bot",
  "main": "bin/cli.js",
  "dependencies": {
    "amqplib": "0.5.1",
    "body-parser": "1.17.2",
    "co": "4.6.0",
    "debug": "amejiarosario/debug#hide-tty-date",
    "express": "4.15.3",
    "gdax": "amejiarosario/gdax-node#heartbeat",
    "moment": "2.18.1",
    "mongodb": "2.2.30",
    "morgan": "1.8.2",
    "rxjs": "5.4.2",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "chai": "4.1.0",
    "husky": "0.14.3",
    "mocha": "3.5.0",
    "nyc": "11.1.0",
    "sinon": "2.4.1"
  },
  "scripts": {
    "test": "ENV=test mocha lib/**/*spec.js",
    "watch": "ENV=test mocha --watch lib/**/*spec.js",
    "e2e": "NODE_TLS_REJECT_UNAUTHORIZED=0 ENV=test mocha test/e2e/{**/,}*{spec,e2e}.js",
    "we2e": "NODE_TLS_REJECT_UNAUTHORIZED=0 ENV=test mocha --watch test/e2e/{**/,}*{spec,e2e}.js",
    "ce2e": "NODE_TLS_REJECT_UNAUTHORIZED=0 ENV=test nyc --reporter=html --reporter=text mocha test/e2e/{**/,}*{spec,e2e}.js",
    "coverage": "nyc --reporter=html --reporter=text npm run test",
    "start": "DEBUG=crybot:* node bin/cli ticker --provider gdax:BTC-USD,ETH-USD --modifier bufferTime:60000",
    "precommit": "npm test",
    "prepush": "npm test",
    "heroku": "DEBUG='crybot:*' DEBUG_DEPTH=6 DEBUG_HIDE_TTY_DATE=true heroku local"
  },
  "keywords": [],
  "author": "Adrian Mejia <me@adrianmejia.com> (http://adrianmejia.com)",
  "license": "ISC",
  "engines": {
    "node": ">8.1.4",
    "npm": ">5.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git://git.heroku.com/crybot.git"
  }
}
